{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch TypeScript",
        "program": "${workspaceFolder}/index.ts",
        "preLaunchTask": "ts-node",
        "outFiles": [
          "${workspaceFolder}/dist/**/*.js"
        ],
        "runtimeArgs": [
          "-r",
          "ts-node/register"
        ],
        "env": {
          "NODE_ENV": "development"
        },
        "console": "integratedTerminal"
      }
    ]
  }
  